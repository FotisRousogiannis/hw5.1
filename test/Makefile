
out:
	./hw5 < test1a.in > test1a.out
	diff -urN test1a.out test1a.std
	./hw5 < test1b.in > test1b.out
	diff -urN test1b.out test1b.std
	./hw5 < test1c.in > test1c.out
	diff -urN test1c.out test1c.std
	./hw5 < test2a.in > test2a.out
	diff -urN test2a.out test2a.std
	./hw5 < test2b.in > test2b.out
	diff -urN test2b.out test2b.std
	./hw5 < test3a.in > test3a.out
	diff -urN test3a.out test3a.std
	./hw5 < test3b.in > test3b.out
	diff -urN test3b.out test3b.std
	./hw5 < test4a.in > test4a.out
	diff -urN test4a.out test4a.std
	./hw5 < test4b.in > test4b.out
	diff -urN test4b.out test4b.std
	./hw5 < test5a.in > test5a.out
	diff -urN test5a.out test5a.std
	./hw5 < test5b.in > test5b.out
	diff -urN test5b.out test5b.std
	./hw5 < test5c.in > test5c.out
	diff -urN test5c.out test5c.std
	./hw5 < test6a.in > test6a.out
	diff -urN test6a.out test6a.std
	
std:
	./hw5 < test1a.in > test1a.std
	./hw5 < test1b.in > test1b.std
	./hw5 < test1c.in > test1c.std
	./hw5 < test2a.in > test2a.std
	./hw5 < test2b.in > test2b.std
	./hw5 < test3a.in > test3a.std
	./hw5 < test3b.in > test3b.std
	./hw5 < test4a.in > test4a.std
	./hw5 < test4b.in > test4b.std
	./hw5 < test5a.in > test5a.std
	./hw5 < test5b.in > test5b.std
	./hw5 < test5c.in > test5c.std
	./hw5 < test6a.in > test6a.std

clean:
